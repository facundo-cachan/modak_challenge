{
  "name": "modak-challenge",
  "version": "2.0.1",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "fix:ios": "cd ios/ && bundle install && bundle exec pod install && cd ..",
    "clean:ios": "cd ios/ && rm -rf build/ Pods/ Podfile.lock && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "update:android": "cd android/ && ./gradlew :app:dependencies && cd ..",
    "update:ios": "cd ios/ && NO_FLIPPER=1 pod install --verbose && cd ..",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest --colors --detectOpenHandles",
    "test:coverage": "jest --colors --coverage",
    "test:watch": "jest --colors --detectOpenHandles --watchAll",
    "ios:release": "npx react-native bundle --entry-file='index.ts' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "android:release": "npx react-native bundle --platform android --dev false --entry-file index.ts --bundle-output android/app/src/main/assets/index.android.bundle && cd android && ./gradlew app:assembleRelease"
  },
  "dependencies": {
    "@notifee/react-native": "^7.8.2",
    "@react-native-community/netinfo": "11.2.1",
    "@react-native-firebase/app": "^18.7.3",
    "@react-native-firebase/messaging": "^18.7.3",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/native-stack": "6.9.17",
    "@react-navigation/stack": "6.3.20",
    "axios": "1.6.2",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-mmkv": "^2.11.0",
    "react-native-permissions": "^4.0.1",
    "react-native-reanimated": "^3.6.1",
    "react-native-render-html": "^6.3.4",
    "react-native-vector-icons": "10.0.3",
    "recoil": "0.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "7.23.3",
    "@babel/runtime": "^7.23.6",
    "@jest/globals": "^29.7.0",
    "@react-native/babel-preset": "^0.74.0",
    "@react-native/eslint-config": "^0.74.0",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.74.0",
    "@react-navigation/routers": "^6.1.9",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.1",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.45",
    "@types/react-native-dotenv": "^0.2.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.1.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-native-dotenv": "^3.4.9",
    "react-native-safe-area-context": "^4.8.1",
    "react-native-screens": "^3.29.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
